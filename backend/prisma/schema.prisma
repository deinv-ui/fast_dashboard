generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @default(cuid())
  firstName String
  lastName  String
  email     String @unique
  gender    String
  country   String
  avatar    String
  createdAt DateTime @default(now())
}
